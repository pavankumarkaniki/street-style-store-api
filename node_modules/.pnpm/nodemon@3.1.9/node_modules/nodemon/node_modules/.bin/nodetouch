#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/react-hooks/sessions/state-hook-part-2/street-style-store-api/node_modules/.pnpm/touch@3.1.1/node_modules/touch/bin/nodetouch.js/node_modules:/home/nxtwave/react-hooks/sessions/state-hook-part-2/street-style-store-api/node_modules/.pnpm/touch@3.1.1/node_modules/touch/bin/node_modules:/home/nxtwave/react-hooks/sessions/state-hook-part-2/street-style-store-api/node_modules/.pnpm/touch@3.1.1/node_modules/touch/node_modules:/home/nxtwave/react-hooks/sessions/state-hook-part-2/street-style-store-api/node_modules/.pnpm/touch@3.1.1/node_modules:/home/nxtwave/react-hooks/sessions/state-hook-part-2/street-style-store-api/node_modules/.pnpm/node_modules:/home/nxtwave/react-hooks/sessions/state-hook-part-2/street-style-store-api/node_modules:/home/nxtwave/react-hooks/sessions/state-hook-part-2/node_modules:/home/nxtwave/react-hooks/sessions/node_modules:/home/nxtwave/react-hooks/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/react-hooks/sessions/state-hook-part-2/street-style-store-api/node_modules/.pnpm/nodemon@3.1.9/node_modules/nodemon/node_modules:/home/nxtwave/react-hooks/sessions/state-hook-part-2/street-style-store-api/node_modules/.pnpm/nodemon@3.1.9/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../touch@3.1.1/node_modules/touch/bin/nodetouch.js" "$@"
else 
  exec node  "$basedir/../../../../../touch@3.1.1/node_modules/touch/bin/nodetouch.js" "$@"
fi
